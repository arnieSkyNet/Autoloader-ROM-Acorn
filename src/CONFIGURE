CONFIGURE_CODE	LDX	#>logon_to_file_server	LDY	#<logon_to_file_server	JSR	OSCLI	LDX	#>config_data_name	LDY	#<config_data_name	JSR	OSCLI	JSR	GET_MACHINE_TYPE	LDA	machine_type	CMP	#01			; BBC	BEQ	:exit_configure	CMP	#02			; BBC+	BEQ	:exit_configure	CMP	#04			; MASTER 128 ET	BEQ	:et_language	LDX	#>configure_language	LDY	#<configure_language	JSR	OSCLI	JMP	:exit_configure:et_language	LDX	#>configure_languageET	LDY	#<configure_languageET	JSR	OSCLI:exit_configure	RTS