DISPLAY_RET_ESC_DEL_OPTION	JSR	DEFAULT_COLOUR	LDA	util_store	CMP	#"D"	BNE	:not_display_objects	JMP	:first_option_display:not_display_objects	LDA	curr_file_syst			; Find out if in DFS	CMP	#04					BEQ	:lets_shade			; If DFS shade some text	LDA	util_store	BEQ	:do_up_user_text:lets_shade	JSR	SHADED_COLOUR:do_up_user_text	LDX	#>up_directory_text		; If DFS shade it	LDY	#<up_directory_text	JSR	PRINT_TEXT	LDX	#>user_root_directory_text	; If DFS shade it	LDY	#<user_root_directory_text	JSR	PRINT_TEXT	LDA	machine_type	CMP	#02			; If > 2 then not on BBC	BPL	:skip_special	LDA	#31			; Must be a BBC B. So must use	JSR	OSWRCH			; a user defined arrow and postion	LDA	#07			; it where the normal arrow would	JSR	OSWRCH			; go. Indent by 6, Line 0 of the	LDA	#0			; current window.	JSR	OSWRCH	LDA	#129			; Get user defined character	JSR	OSWRCH			; Print user defined character:skip_special	JSR	DEFAULT_COLOUR	LDA	util_store	BEQ	:do_not_shade_options_one	JSR	SHADED_COLOUR:do_not_shade_options_one	LDX	#>select_util_text	LDY	#<select_util_text	JSR	PRINT_TEXT	LDX	#>other_move_keys_text	LDY	#<other_move_keys_text	JSR	PRINT_TEXT:first_option_display	JSR	DEFAULT_COLOUR	LDA	util_store	CMP	#"D"	BNE	:standard_40	LDA	#25	STA	Temp+1	JMP	:carry_on_normally:standard_40	LDA	#40			; Indent value of 40	STA	Temp+1			; Save indent value:carry_on_normally	JSR	POS_MIDDLE_SCREEN	; Position cursor middle screen	LDA	#00			; Position at Line number	JSR	OSWRCH	LDA	util_store	CMP	#"D"	BNE	:normal_escape_text	LDX	#>cl_save	LDY	#<cl_save	JMP	:print_the_escape_text:normal_escape_text	LDX	#>escape_option_text	LDY	#<escape_option_text:print_the_escape_text	JSR	PRINT_TEXT	JSR	POS_MIDDLE_SCREEN	LDA	#01			; Position at line 1	JSR	OSWRCH	LDX	#>select_option_text	LDY	#<select_option_text	JSR	PRINT_TEXT       LDA	#31 			; Position cursor to print last       JSR     OSWRCH 			; read object number (<A to ?>)	LDA	Temp+1			; Get indent number	CLC	ADC	#06			; Add 6 to indent number       JSR     OSWRCH			; Position cursor at indent pos.       LDA	#01			; Load line number       JSR     OSWRCH 			; Position cursor at line number.       LDA     num_keys		; Get number of keys can press       TAY				; Transfer number to Y       DEY				; Take 1 of Y       LDA     key_storage,Y		; Get 2nd last number in buffer       JSR     OSWRCH			; Print final object letter	JSR	DISPLAY_SELECT_UTILITY_OPTIONS	LDA	#02	STA	Temp       LDA     more_ret        CMP	#0       BEQ	:no_ret_opt       LDA	#31       JSR     OSWRCH        LDA	Temp+1       JSR     OSWRCH        LDA	#02       JSR     OSWRCH	LDA	Temp	PHA	LDA	Temp+1	PHA	JSR	INVERSE_COLOUR	LDX	#>return_option_text	LDY	#<return_option_text	JSR	PRINT_TEXT	LDA	screen_num	JSR	PRINT_NUMBER	JSR	DEFAULT_COLOUR	PLA	STA	Temp+1	PLA		STA	Temp	LDA	#03	STA	Temp:no_ret_opt	LDA	util_store       BEQ	:shade_utils	CMP	#"D"	BEQ	:end_esc_del_ret_option		JSR	SHADED_COLOUR:shade_utils       LDA	#31       JSR     OSWRCH        LDA	Temp+1       JSR     OSWRCH        LDA	Temp       JSR     OSWRCH       LDX	#>delete_option_text       LDY	#<delete_option_text       JSR     PRINT_TEXT	INC	Temp	LDA	#31       JSR     OSWRCH        LDA	Temp+1       JSR     OSWRCH        LDA	Temp       JSR     OSWRCH       LDX	#>copy_option_text       LDY	#<copy_option_text       JSR     PRINT_TEXT	INC	Temp	LDA	#31       JSR     OSWRCH        LDA	Temp+1       JSR     OSWRCH        LDA	Temp       JSR     OSWRCH       LDX	#>disc_option_text       LDY	#<disc_option_text       JSR     PRINT_TEXT	INC	Temp       LDA	#31       JSR     OSWRCH        LDA	Temp+1       JSR     OSWRCH        LDA	Temp       JSR     OSWRCH       LDX	#>info_option_text       LDY	#<info_option_text       JSR     PRINT_TEXT	INC	Temp       LDA	#31       JSR     OSWRCH        LDA	Temp+1       JSR     OSWRCH        LDA	Temp       JSR     OSWRCH       LDX	#>help_option_text       LDY	#<help_option_text       JSR     PRINT_TEXT	INC	Temp:end_esc_del_ret_option       RTSPOS_MIDDLE_SCREEN	LDA	#31	JSR	OSWRCH	LDA	Temp+1	JSR	OSWRCH	RTSDISPLAY_SELECT_UTILITY_OPTIONS	LDA	#31	JSR	OSWRCH	LDA	util_store	CMP	#"D"	BNE	:skip_indent_alter	LDA	#37	JMP	:carry_ondisp_select:skip_indent_alter	LDA	#52:carry_ondisp_select	JSR	OSWRCH	LDA	#01	JSR	OSWRCH       LDA     util_store       CMP	#127       BEQ     :delete_option_only       CMP	#"3"        BEQ     :rename_option_only	LDX	#>select_object_text	LDY	#<select_object_text	JSR	PRINT_TEXT       JMP     :other_options:delete_option_only       LDX	#>delete_object_text       LDY	#<delete_object_text       JSR     PRINT_TEXT       JMP     :other_options:rename_option_only       LDX	#>rename_object_text       LDY	#<rename_object_text       JSR     PRINT_TEXT:other_options       RTS; To make a NON Hinwick REM out the following:-;;	LDX	#>staff_util_text	3 Lines;