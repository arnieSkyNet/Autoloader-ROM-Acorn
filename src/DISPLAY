Å..........*.......*.......*......*.......*.......*.......*.......*.......*.<PRINT_TEXT			        STX     text_add         STY     text_add+1         LDY	#0:print_text_loop        LDA     (text_add),Y         CMP	#&FF        BEQ     :quit_print_text        CMP	#13        BNE     :not_return_character        JSR     OSNEWL         INY        JMP     :print_text_loop:not_return_character        JSR     OSWRCH         INY        JMP     :print_text_loop:quit_print_text        RTSOPT_WINDOW        LDA	#26        JSR     OSWRCH         LDA	#31        JSR     OSWRCH        LDA	#0        JSR     OSWRCH 	LDA	util_store	CMP	#"D"	BNE	:get_line16	LDA	screen_mode	JMP	:do_bar_option:get_line16	LDA	#16:do_bar_option        JSR     OSWRCH         JSR     PRINT_LONG_BAR        LDX	#>bottom_window_data        LDY	#<bottom_window_data	LDA	util_store	CMP	#"D"	BNE	:define_opt_window	JSR	READ_CURRENT_MODE	CPY	#0	BEQ	:do_alt_opt_data	CPY	#128	BEQ	:do_alt_opt_data        LDX	#>bottom_window_data        LDY	#<bottom_window_data	JMP	:define_opt_window:do_alt_opt_data	LDX	#>alt_opt_wind	LDY	#<alt_opt_wind:define_opt_window        JSR     PRINT_TEXT        RTSPRINT_LONG_BAR			JSR	DEFAULT_COLOUR        LDY	#80        LDA	#128:bar_loop        JSR     OSWRCH         DEY        CPY	#0        BNE     :bar_loop        RTSPRINT_BLOCK_NAME	LDA	param_blk,Y	JSR	OSWRCH	INY	CMP	#32	BMI	:print_block	CPY	Temp	BNE	PRINT_BLOCK_NAME:print_block	RTSPRINT_NUMBER	STA	Temp	LDA	util_store	PHA	LDA	#0	STA 	Temp+1	TAX:digitloop	LDA	#0	STA	util_store	LDA	Temp	SEC:another	STA	Temp	INC	util_store	SBC	table,X	BCS	:another	DEC	util_store	LDA	util_store	BNE	:prntdigit	LDA	Temp+1	BEQ	:nextdecade:prntdigit	LDA	util_store	ORA	#&30	JSR	OSWRCH	INC	Temp+1:nextdecade	INX	CPX	#2	BNE	:digitloop	LDA	Temp	ORA	#&30	JSR	OSWRCH	PLA	STA	util_store	RTSPRINT_FS_PS_NUMBER	JSR	GET_FS_PS_NUMBER	JSR	INVERSE_COLOUR	LDA	cont_blk+2	JSR	PRINT_NUMBER	LDA	#"."	JSR	OSWRCH	LDA	cont_blk+1	JSR	PRINT_NUMBER	JSR	DEFAULT_COLOUR	RTSTOP_HEADER			STX	text_pos	STY	text_pos+1        LDA	#31         JSR     OSWRCH         LDA	#0         JSR     OSWRCH 	JSR	OSWRCH        JSR     PRINT_LONG_BAR 	JSR	INVERSE_COLOUR	LDX	text_pos	LDY	text_pos+1	JSR	PRINT_TEXT	JSR	DEFAULT_COLOUR        LDX	#>main_window_data        LDY	#<main_window_data        JSR     PRINT_TEXT         RTSINVERSE_COLOUR		        LDX	#>invert_colour_data        LDY	#<invert_colour_data        JSR     PRINT_TEXT         RTSSHADED_COLOUR        LDA	#84	STA	&D2	STA	&D3		RTSDEFAULT_COLOUR		        LDX	#>default_colour_data        LDY	#<default_colour_data        JSR     PRINT_TEXT         RTSPRINT_NA	JSR	INVERSE_COLOUR	LDX	#>non_applic_data	LDY	#<non_applic_data	JSR	PRINT_TEXT	JSR	DEFAULT_COLOUR	RTS