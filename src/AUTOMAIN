Å..........*.......*.......*......*.......*.......*.......*.......*.......*.<workspace	INCLUDE	workspace	; Zero page & page 9 workspace	ORG	&8000	MSW	&FFFFsideways	INCLUDE Romheader	; Rom Header fileSET_SCREEN	LDA	#03			; Make sure PS / Port is Off	JSR	OSWRCH 	LDA	#21	LDX	#0	LDY	#0	JSR	OSBYTE			; Flush keyboard buffer 	LDA	#229	LDX	#1	LDY	#0	JSR	OSBYTE			; Disable Escape Action	LDA	#213	LDX	#101	LDY	#0	JSR	OSBYTE	 		; Set bell frequency	LDA	#214	LDX	#1	JSR	OSBYTE			; Reset bell duration to 1	JSR	MODE131			; Change into MODE 131/3	JSR	FILING_SYSTEM		; Get current filing system	JSR	GET_MACHINE_TYPE	; Find out what machine this is	JSR	SET_KEY_RATES		; Save key rates & set to 0	JSR	CLEAR_DATA_BUFFER	; Clear main data buffer	JSR	CHECK_FOR_REPORT_MODE	; Check if in Comments/Assessment	JSR	SET_INTERNAL_CLOCK	; Set internal clockPROGRAM_START	LDA	#0			; Set A to 0	STA	util_store		; Save value to util_store = 0	STA	file_num		; Save value to file_num = 0	STA	screen_num		; Save value to screen_num = 0	STA	chime_bit		; Disable chimer	STA	objects_status		; Indicates no objects read	JSR     MODE131			; Change to Mode 131/3	LDX	#>autoload_heading_text	LDY	#<autoload_heading_text	JSR     TOP_HEADER			JSR     DISPLAY_OBJECTS_OPTIONScheck_menu_keys	INCLUDE menu_check	; Check if key pressed and dot it!:exit_program_start	 	LDA	#229	LDX	#0	LDY	#0	JSR	OSBYTE			; Enale Escape Action        LDA	#18        LDX	#0        LDY	#0        JSR     OSBYTE			; Clear Function key buffers	JSR     RESTORE			; Restore keyboard rates as found	JSR	CHIME_OFF_CODE		; Reset Timer events to entry VECs	LDY	#32			; Reset zero page values as found.:al_zp_puller_loop	PLA	STA	&6E,Y	DEY	CPY	#0	BNE	:al_zp_puller_loop	JSR	EXIT_FROM_AUTOLOADER:exit_big_time	PLA	TAX	PLA	TAY	LDA	#0        RTS; This is the end of the main program and the beginning of the subroutinesdisplay_routine	INCLUDE main_disp	; This is the main display routinefile_loading	INCLUDE	loaders	; To load files into Word Prossy'sget_name_osword	INCLUDE	get_name	; This gets a string key/switchestransfer_routs	INCLUDE	transfer	; To copy files between FS'skeyboard_routs	INCLUDE	check_key	; To check keys and switchesobject_display	INCLUDE	objects	; Read/object/type/displayerror_routines	INCLUDE	intercepts	; Error handling routinescontrol_blks	INCLUDE	cont_blks	; Cont_blks OSwordsdisplaying	INCLUDE display	; Display print codesesc_options	INCLUDE	options	; Display other optionsminor_stuff	INCLUDE minor		; Minor routinestime_date_clock	INCLUDE time_date	; Does Time/Date for any machineselect_a_disc	INCLUDE	choosedisc	; Selects disc in any CFShelp_objects	INCLUDE	help_part	; This is the help objects sectionget_routines	INCLUDE	get_codes	; These are the get codesall_logon_codes	INCLUDE	logon_code 	; All code for Logging onconfigure_code	INCLUDE	configure	; To allow configuration by userall_autos_data	INCLUDE	autodata	; Autoloads data and texthinwick_data	INCLUDE	hh_data	; Data used only at Hinwickhinwick_code	INCLUDE	hh_code	; Code used only at Hinwick