Å..........*.......*.......*......*.......*.......*.......*.......*.......*.<MAIN_HELP_UTILITY	JSR	DISPLAY_OBJECTS_OPTIONS:skip_display_same	LDA	key_press	CMP	#27	BNE	:not_escape_help	JSR	RESET_PAGE_OBJECT_NUMBERS	RTS:not_escape_help	JSR	GET_OBJECT_CHOSEN	JSR	OBJECT_TYPE	LDX	#>help_data	LDY	#<help_data	STX	Temp	STY	Temp+1	LDY	#0:move_help	LDA	(Temp),Y	STA	param_blk,Y	INY	CMP	#13	BNE	:move_help	DEY	LDA	#"."	STA	param_blk,Y	INY	LDX	#0:help_object_move	LDA	name_store,X	STA	param_blk,Y	INY	INX	CPX	#11	BEQ	:exit_help_loop	CMP	#32	BEQ	:exit_help_loop	CMP	#&FF	BNE	:help_object_move:exit_help_loop	DEY	LDA	#13	STA	param_blk,Y	LDX	#>param_blk	LDY	#<param_blk	STX	cont_blk	STY	cont_blk+1	LDX	#>cont_blk	LDY	#<cont_blk	LDA	#05	JSR	OSFILE	CMP	#1	BEQ	:there_is_help_file:no_help_text_entry	JSR	OPT_WINDOW	LDX	#>no_help_text	LDY	#<no_help_text	JSR	PRINT_TEXT	JSR	OSRDCH	JSR	RESET_PAGE_OBJECT_NUMBERS	JMP	MAIN_HELP_UTILITY:there_is_help_file	LDX	#>main_window_data	LDY	#<main_window_data	JSR	PRINT_TEXT	LDX	#>param_blk	LDY	#<param_blk	LDA	#&40	JSR	OSFIND	CMP	#0	BEQ	:no_help_text_entry	TAY	STA	cfs_tran	LDA	#0	STA	Temp:get_byte_loop	JSR	OSBGET	BCS	:end_of_file	JSR	OSASCI	CMP	#13	BEQ	:ret_char	JMP	:get_byte_loop:ret_char	INC	Temp	LDA	Temp	CMP	#21	BNE	:get_byte_loop	LDA	#0	STA	Temp		JSR	INFO_SPACE_PRESS	LDY	cfs_tran	JMP	:get_byte_loop:end_of_file	LDA	#0	JSR	OSFIND	JSR	INFO_SPACE_PRESS	JSR	RESET_PAGE_OBJECT_NUMBERS	LDA	#0	STA	file_num	JMP	MAIN_HELP_UTILITYINFO_SPACE_PRESS	JSR	OSNEWL	JSR	INVERSE_COLOUR	JSR	PRESS_SPACE_BAR	LDA	#12	JSR	OSWRCH	RTSCHECK_FOR_MENUDATA	LDX	#>mcmenu_data_name	LDY	#<mcmenu_data_name	STX	Temp	STY	Temp+1	LDY	#0:move_mcmenu_data_name	LDA	(Temp),Y	STA	param_blk,Y	INY	CMP	#13	BNE	:move_mcmenu_data_name	JSR	OBJECT_TYPE	RTS